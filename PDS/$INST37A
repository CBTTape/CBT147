//A0611AR  JOB 01,ARCHIVER,CLASS=A,MSGLEVEL=(1,1),
//         MSGCLASS=X,TIME=30,NOTIFY=&SYSUID,REGION=2M
//*
//*     PDSDOC *INSTALL ARCHIVER VERSION 6 ON MVS3.8J................*
//*
//ARCHASM  PROC  M=
//ASM      EXEC  PGM=IFOX00,
//         PARM='TERM,DECK,NOOBJ,NORLD,XREF(SHORT)'
//SYSPRINT DD  SYSOUT=*
//SYSTERM  DD  SYSOUT=*
//SYSLIB   DD  DSN=&&MACLIB,DISP=(OLD,PASS)
//         DD  DSN=A0611.ARCHIVER.ASM,DISP=SHR
//         DD  DSN=SYS1.AMODGEN,DISP=SHR
//         DD  DSN=SYS1.MACLIB,DISP=SHR
//SYSUT1   DD  UNIT=SYSALLDA,SPACE=(CYL,(3,1))
//SYSUT2   DD  UNIT=SYSALLDA,SPACE=(CYL,(3,1))
//SYSUT3   DD  UNIT=SYSALLDA,SPACE=(CYL,(3,1))
//SYSIN    DD  DSN=A0611.ARCHIVER.ASM(&M),DISP=SHR
//SYSPUNCH DD  DSN=A0611.ARCHIVER.OBJ(&M),DISP=SHR
//         PEND
//*
//STEP01  EXEC PGM=IEBUPDTE,PARM=NEW
//SYSPRINT DD  SYSOUT=*
//SYSUT2   DD  DSN=&&MACLIB,UNIT=3390,SPACE=(800,(3,2,2)),
//             DISP=(NEW,PASS),DCB=(RECFM=FB,LRECL=80,BLKSIZE=27920)
//SYSIN    DD  *
./ ADD NAME=AMODE
         MACRO
&NAME    AMODE
         MEND
./ ADD NAME=RMODE
         MACRO
&NAME    RMODE
         MEND
./ ENDUP
/*
//*
//STEP02  EXEC PGM=IDCAMS
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
 DELETE A0611.ARCHIVER.OBJ PURGE
 DELETE A0611.ARCHIVER.LOAD PURGE
 SET LASTCC=0
 SET MAXCC=0
