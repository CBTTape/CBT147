//SYSUT1   DD    UNIT=SYSALLDA,SPACE=(CYL,(3,1))
//SYSLIB   DD    DSN=A0611.ARCHIVER.OBJ,DISP=SHR
//SYSLMOD  DD    DSN=A0611.ARCHIVER.LOAD,DISP=SHR
//SYSLIN   DD    *
 INCLUDE SYSLIB(ARCHMAIN)
 INCLUDE SYSLIB(ARCHTABS)
 ENTRY ARCHMAIN
 NAME ARCHIVER(R)
//LINK2    EXEC  PGM=IEWL,PARM='LIST,LET,XREF,MAP'
//SYSPRINT DD    SYSOUT=*
