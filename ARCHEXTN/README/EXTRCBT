//EXTNCBT  JOB 'ME',MSGCLASS=A,MSGLEVEL=(1,1),CLASS=A,NOTIFY=&SYSUID,
//        USER=HERCEL,PASSWORD=CUL8TR
//*
//S1 EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
 DELETE 'HERCEL.ARCHEXTN.PDSXMI' NONVSAM PURGE
 DELETE 'HERCEL.ARCHEXTN.CBTXMI' NONVSAM PURGE
 SET LASTCC=0
 SET MAXCC=0
//*
//S2   EXEC PGM=IEFBR14
//DD1      DD DSN=HERCEL.ARCHEXTN.PDSXMI,DISP=(NEW,CATLG,DELETE),
//            UNIT=SYSDA,VOL=SER=EVOL00,SPACE=(CYL,(3,1,10)),
//            DCB=(DSORG=PO,RECFM=FB,LRECL=80,BLKSIZE=3120)
//DD2      DD DSN=HERCEL.ARCHEXTN.CBTXMI,DISP=(NEW,CATLG,DELETE),
//            UNIT=SYSDA,VOL=SER=EVOL00,SPACE=(CYL,(3,1)),
//            DCB=(DSORG=PS,RECFM=FB,LRECL=80,BLKSIZE=3120)
//*
//IKJACCNT EXEC PGM=IKJEFT01,REGION=4096K
//SYSTSPRT DD SYSOUT=*
//SYSTSIN  DD *
 PROFILE PREFIX(HERCEL)
 TRANSMIT DATASET(ARCHEXTN.ASM)     OUTDATASET(ARCHEXTN.PDSXMI(ASM))
 TRANSMIT DATASET(ARCHEXTN.CNTL)    OUTDATASET(ARCHEXTN.PDSXMI(README))
 TRANSMIT DATASET(ARCHEXTN.INCLLIB) OUTDATASET(ARCHEXTN.PDSXMI(INCLLIB))
 TRANSMIT DATASET(ARCHEXTN.JCL)     OUTDATASET(ARCHEXTN.PDSXMI(JCL))
 TRANSMIT DATASET(ARCHEXTN.LOADLIB) OUTDATASET(ARCHEXTN.PDSXMI(LOADLIB))
 TRANSMIT DATASET(ARCHEXTN.MACLIB)  OUTDATASET(ARCHEXTN.PDSXMI(MACLIB))
 TRANSMIT DATASET(ARCHEXTN.NCALIB)  OUTDATASET(ARCHEXTN.PDSXMI(NCALIB))
 TRANSMIT DATASET(ARCHEXTN.PLI)     OUTDATASET(ARCHEXTN.PDSXMI(PLI))
 TRANSMIT DATASET(ARCHEXTN.PROCLIB) OUTDATASET(ARCHEXTN.PDSXMI(PROCLIB))
 TRANSMIT DATASET(ARCHEXTN.CNTL)    OUTDATASET(ARCHEXTN.PDSXMI(README))
 TRANSMIT DATASET(ARCHEXTN.PDSXMI)  OUTDATASET(ARCHEXTN.CBTXMI)
